datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model GasBill {
  amount    Float?
  createdAt DateTime  @default(now())
  date      DateTime?
  id        String    @id @default(cuid())
  provider  String?
  updatedAt DateTime  @updatedAt
  usage     Int?
}

model WaterBill {
  amount    Float?
  createdAt DateTime  @default(now())
  date      DateTime?
  id        String    @id @default(cuid())
  provider  String?
  updatedAt DateTime  @updatedAt
  usage     Int?
}

model ElectricBill {
  amount    Float?
  createdAt DateTime  @default(now())
  date      DateTime?
  id        String    @id @default(cuid())
  provider  String?
  updatedAt DateTime  @updatedAt
  usage     Int?
}
